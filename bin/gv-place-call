#!/usr/bin/env ruby -W1
# coding: UTF-8
$:.unshift(File.join(File.dirname(__FILE__), '..', 'lib'))
require 'gvoice-ruby'
include GvoiceRuby

voicebox = GvoiceRuby::Client.new

this_phone = { :outgoing_number => "3088720071", :forwarding_number => "3088721257" }

puts "Call connected!" if voicebox.call(this_phone)